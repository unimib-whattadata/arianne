{
  "$schema": "https://turborepo.org/schema.json",
  "globalEnv": [
    "DATABASE_URL",
    "NEXT_PUBLIC_TRPC_LOGGER_ENABLED",
    "COOKIE_SECRET",
    "NEXT_PUBLIC_APP_URL",
    "NEXT_PUBLIC_PATIENT_URL",
    "NEXT_PUBLIC_THERAPIST_URL",
    "NEXT_PUBLIC_LANDING_URL",
    "NEXT_PROJECT_PROJECT_ID",
    "NEXT_PUBLIC_WSS_URL",
    "REDIS"
  ],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "CI",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "npm_lifecycle_event"
  ],
  "tasks": {
    "//#clean": {
      "cache": false
    },
    "build": {
      "outputs": [
        ".cache/tsbuildinfo.json",
        "dist/**"
      ]
    },
    "clean": {
      "cache": false
    },
    "db:create-only": {
      "cache": false,
      "interactive": true
    },
    "dev": {
      "cache": false,
      "interactive": true,
      "persistent": false
    },
    "expo:install": {
      "cache": false
    },
    "format": {
      "outputLogs": "new-only",
      "outputs": [
        ".cache/.prettiercache"
      ]
    },
    "lint": {
      "dependsOn": [
        "^topo",
        "^build"
      ],
      "outputs": [
        ".cache/.eslintcache"
      ]
    },
    "db:generate": {
      "cache": false,
      "interactive": true
    },
    "db:migrate": {
      "cache": false,
      "interactive": true
    },
    "db": {
      "cache": false,
      "interactive": true
    },
    "db:studio": {
      "cache": false,
      "persistent": true
    },
    "db:utils": {
      "cache": false,
      "interactive": true
    },
    "topo": {
      "dependsOn": [
        "^topo"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "^topo",
        "^build"
      ],
      "outputs": [
        ".cache/tsbuildinfo.json"
      ]
    }
  },
  "ui": "tui"
}